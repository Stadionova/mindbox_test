{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "entities": [
        "src/entities"
      ]
    },
    "target": "ES6",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}